default menu.c32
prompt 0
timeout 200
ONTIMEOUT 2

menu title ########## PXE Boot Menu ########## 

label 1
  menu label ^1) Install Centos7-x64
  kernel centos7/vmlinuz
  append initrd=centos7/initrd.img method=http://192.168.0.3/pub devfs=nomount

label 2
  menu label ^2) Boot from local media
  localboot 0xffff

